{
  "name": "exifier",
  "description": "an EXIF viewer",
  "version": "1.0.0",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "expo": "~44.0.0",
    "expo-dev-client": "~0.8.4",
    "expo-image-picker": "~12.0.1",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.6",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^7.0.0",
    "prettier": "2.5.1",
    "typescript": "~4.3.5"
  },
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "build:all": "eas build --profile development --platform all",
    "build:android": "eas build --profile development --platform android",
    "build:ios": "eas build --profile development --platform ios",
    "commit": "npx cz",
    "ios": "expo run:ios",
    "start:dev": "ENVIRONMENT=development expo start --dev-client",
    "start:stg": "ENVIRONMENT=staging expo start --dev-client",
    "web": "expo start --web"
  }
}
